<div class="row">
    <div class="form-group">
        <label class="col-sm-3 control-label">委托单位</label>
        <div class="col-sm-6">
            <label class="col-md-10">{{data.client_unit}}</label>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label">邮政编码</label>
        <div class="col-sm-6">
            <label class="col-md-10">{{data.client_code}}</label>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label">联系地址</label>
        <div class="col-sm-6">
            <label class="col-md-10">{{data.client_address}}</label>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label">联系电话</label>
        <div class="col-sm-6">
            <label class="col-md-10">{{data.client_tel}}</label>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label">联系人</label>
        <div class="col-sm-6">
            <label class="col-md-10">{{data.client}}</label>
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label">传真电话</label>
        <div class="col-sm-6">
            <label class="col-md-10">{{data.client_fax}}</label>
        </div>
    </div>
    <div class="form-group">
        <div class="text-center">
            <a class="btn btn-default" data-dismiss="modal">关 闭</a>
        </div>
    </div>
</div>